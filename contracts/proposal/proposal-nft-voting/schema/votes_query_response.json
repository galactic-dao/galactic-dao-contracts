{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VotesQueryResponse",
  "type": "object",
  "required": [
    "votes"
  ],
  "properties": {
    "votes": {
      "description": "Voted option IDs in the same order as the token IDs provided in the query, null if no vote exists",
      "type": "array",
      "items": {
        "type": [
          "integer",
          "null"
        ],
        "format": "uint16",
        "minimum": 0.0
      }
    }
  }
}
